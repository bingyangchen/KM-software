		{"id":"b60c6beaac4648f5","type":"text","text":"Elastic Compute Cloud","x":147,"y":99,"width":244,"height":58},
		{"id":"184100abb087ec64","type":"text","text":"Instance Types","x":-210,"y":17,"width":178,"height":58},
		{"id":"4ab6a42e5b0c6b60","type":"text","text":"Each instance type is grouped under a instance family.","x":-273,"y":-177,"width":303,"height":99},
